                steps {
                    echo 'Start building'
                }
            }
            stage('Test') {
                steps {
                    echo 'This is test'
                }
            }
            stage('Deploy') {
                steps {
                    echo 'complete deploy'
                }
            }
        }
    }
	
	
	
	
